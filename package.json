    "qs": "^6.10.1",
    "readline": "^1.3.0"
  },
  "autoInstall": false,
  "devDependencies": {
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.0.23",
    "@typescript-eslint/eslint-plugin": "^3.10.0",
    "@typescript-eslint/parser": "^3.10.0",
    "eslint": "^7.7.0",
    "ts-node": "^8.10.2",
    "typedoc": "^0.20.35",
